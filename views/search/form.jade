.container
  .row
    .col-3
      img#logo(src="/img/two-keys.png")
    .col-9
      form.shadow#searchForm(action="/s", method="get", role="form")
        .inputs
          input#searchBox(name="q", type="text", autofocus="true", value="#{userQuery? userQuery:''}", placeholder="e.g. 'python new list'")
          button#searchButton(type="submit")